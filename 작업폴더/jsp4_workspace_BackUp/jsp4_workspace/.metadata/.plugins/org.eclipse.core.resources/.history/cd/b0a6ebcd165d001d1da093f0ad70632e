<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="Reservation">

	<select id="searchRoom" parameterType="hashmap" resultType="reservationdto">
		SELECT * FROM room WHERE R_ID NOT in(
            SELECT R_ID 
            FROM RESERVATION r 
            WHERE RES_CHECKIN = #{RES_CHECKIN} 
            OR  (RES_CHECKIN &lt; #{RES_CHECKIN}  AND RES_CHECKOUT &gt; #{RES_CHECKIN})
            OR     (RES_CHECKIN &gt; #{RES_CHECKIN}  AND RES_CHECKIN &lt; #{RES_CHECKOUT} ))
	</select>
</mapper>